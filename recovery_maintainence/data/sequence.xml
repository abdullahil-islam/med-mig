        <record id="seq_maintenance_entry" model="ir.sequence">
            <field name="name">Maintenance Entry</field>
            <field name="code">maintenance.status.seq</field>
            <field name="prefix">MS-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_maintenance_entry_line_amc" model="ir.sequence">
            <field name="name">Maintenance Entry Line AMC</field>
            <field name="code">maintenance.line.amc.seq</field>
            <field name="prefix">AMC-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_maintenance_entry_line_oncall" model="ir.sequence">
            <field name="name">Maintenance Entry Line OnCall</field>
            <field name="code">maintenance.line.oc.seq</field>
            <field name="prefix">OC-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_maintenance_entry_line_repair" model="ir.sequence">
            <field name="name">Maintenance Entry Line AMC</field>
            <field name="code">maintenance.line.r.seq</field>
            <field name="prefix">R-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
