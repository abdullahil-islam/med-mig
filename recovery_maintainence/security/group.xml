<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record model="ir.module.category" id="maintenance_entry_category">
            <field name="name">Maintenance Entry</field>
            <field name="sequence">1</field>
        </record>

        <record id="group_maintenance_entry_user" model="res.groups">
            <field name="name">User</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="category_id" ref="recovery_maintainence.maintenance_entry_category"/>
        </record>

        <record id="group_maintenance_entry_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="recovery_maintainence.maintenance_entry_category"/>
            <field name="implied_ids" eval="[(4, ref('recovery_maintainence.group_maintenance_entry_user'))]"/>
        </record>
    </data>
</odoo>
